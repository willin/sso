{
  "private": true,
  "name": "sso.willin.wang",
  "version": "0.0.1",
  "description": "R3mix of Willin.Wang",
  "author": "Willin Wang <willin@willin.org> (https://willin.wang/)",
  "license": "GPL-3.0",
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "remix dev --manual -c \"npm run start\"",
    "start": "wrangler pages dev --compatibility-date=2023-08-15 ./public --d1=demo --local",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s && git add CHANGELOG.md",
    "typecheck": "tsc",
    "db:init": "wrangler d1 migrations list sso",
    "db:migration": "npx wrangler d1 migrations apply sso",
    "prepare": "husky install"
  },
  "dependencies": {
    "@remix-run/cloudflare": "^2.0.0",
    "@remix-run/cloudflare-pages": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "classnames": "^2.3.2",
    "dayjs": "^1.11.10",
    "isbot": "^3.6.13",
    "nanoid": "^5.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.5.1",
    "remix-auth-afdian": "^1.0.0",
    "remix-auth-github": "^1.6.0",
    "remix-i18n": "^1.0.1",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230914.0",
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "conventional-changelog-cli": "^2.2.2",
    "daisyui": "^4.3.1",
    "drizzle-kit": "^0.19.13",
    "eslint": "^8.49.0",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "wrangler": "^3.8.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "*.+(js|jsx|json|yml|yaml|css|less|scss|ts|tsx|md|graphql|mdx|vue)": [
      "prettier --write"
    ],
    "*.+(js|jsx|ts|tsx|vue)": [
      "eslint --fix"
    ]
  }
}
