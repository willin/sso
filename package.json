    "nanoid": "^5.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-auth": "^3.5.1",
    "remix-auth-afdian": "^1.0.0",
    "remix-auth-github": "^1.6.0",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20230914.0",
    "@remix-run/dev": "^2.0.0",
